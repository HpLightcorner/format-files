{
    "name": "jbockle-format-all-workspace-files",
    "displayName": "Format ALL Workspace Files",
    "description": "",
    "version": "0.0.2",
    "publisher": "jbockle",
    "repository": {
        "type": "git",
        "url": "https://github.com/jbockle/format-all-workspace-files.git"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:editor.action.formatAllWorkspaceFiles"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "editor.action.formatAllWorkspaceFiles",
                "title": "Format ALL Workspace Files"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
